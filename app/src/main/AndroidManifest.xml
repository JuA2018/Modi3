<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.techtown.modi">

    <application
        android:allowBackup="true"
        android:icon="@drawable/downloadicon"
        android:label="MoDi"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".signupActivity" />
        <activity android:name=".writediaryActivity"
            android:windowSoftInputMode="adjustResize"/>
        <activity android:name=".TabmanagementActivity" />
        <activity android:name=".ListdiaryActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".ImageActivity"></activity>
    </application>

</manifest>